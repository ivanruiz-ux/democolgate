{
  "welcomeFile": "/ui5/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/ui5/(.*)$",
      "destination": "ui5_static",
      "target": "/$1",
      "authenticationType": "none"
    },
    {
      "source": "^/api/(.*)$",
      "destination": "cpi_bapi",
      "target": "/http/$1",
      "authenticationType": "none",
      "csrfProtection": false
    }
  ]
}
